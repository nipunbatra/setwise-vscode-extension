{
    "MCQ Question": {
        "prefix": "mcq",
        "body": [
            "{",
            "    \"question\": r\"${1:Your question here?}\",",
            "    \"options\": [",
            "        r\"${2:Option A}\",",
            "        r\"${3:Option B}\",",
            "        r\"${4:Option C}\",",
            "        r\"${5:Option D}\"",
            "    ],",
            "    \"answer\": r\"${2:Option A}\",",
            "    \"marks\": ${6:1}",
            "},"
        ],
        "description": "Insert MCQ question template"
    },
    "Subjective Question": {
        "prefix": "subjective",
        "body": [
            "{",
            "    \"question\": r\"${1:Your subjective question here.}\",",
            "    \"answer\": r\"${2:Sample answer or solution steps.}\",",
            "    \"marks\": ${3:5}",
            "},"
        ],
        "description": "Insert subjective question template"
    },
    "Template Question": {
        "prefix": "template",
        "body": [
            "{",
            "    \"template\": r\"${1:Question with {{ variable }} placeholder}\",",
            "    \"variables\": [",
            "        {\"${2:variable}\": ${3:value1}, \"answer\": \"${4:Answer for value1}\"},",
            "        {\"${2:variable}\": ${5:value2}, \"answer\": \"${6:Answer for value2}\"}",
            "    ],",
            "    \"marks\": ${7:3}",
            "},"
        ],
        "description": "Insert template question with variables"
    },
    "Math Expression": {
        "prefix": "math",
        "body": [
            "$${1:x^{2} + y^{2} = z^{2}}$"
        ],
        "description": "Insert LaTeX math expression"
    },
    "Fraction": {
        "prefix": "frac",
        "body": [
            "\\\\frac{${1:numerator}}{${2:denominator}}"
        ],
        "description": "Insert LaTeX fraction"
    },
    "Chemical Formula": {
        "prefix": "chem",
        "body": [
            "${1:H}$_{${2:2}}$${3:O}"
        ],
        "description": "Insert chemical formula with subscripts"
    },
    "Physics Equation": {
        "prefix": "physics",
        "body": [
            "{",
            "    \"question\": r\"${1:A projectile is launched at {{ angle }}° with velocity {{ velocity }} m/s. Calculate the range.}\",",
            "    \"variables\": [",
            "        {\"angle\": ${2:30}, \"velocity\": ${3:20}, \"answer\": \"${4:Range = v²sin(2θ)/g = 35.35 m}\"},",
            "        {\"angle\": ${5:45}, \"velocity\": ${6:25}, \"answer\": \"${7:Range = v²sin(2θ)/g = 63.78 m}\"}",
            "    ],",
            "    \"marks\": ${8:5}",
            "},"
        ],
        "description": "Insert physics question template"
    },
    "Chemistry Equation": {
        "prefix": "chemequation",
        "body": [
            "{",
            "    \"question\": r\"Balance the equation: ${1:Al + Fe$_2$O$_3$ → Al$_2$O$_3$ + Fe}\",",
            "    \"answer\": r\"${2:2Al + Fe$_2$O$_3$ → Al$_2$O$_3$ + 2Fe}\",",
            "    \"marks\": ${3:3}",
            "},"
        ],
        "description": "Insert chemistry equation template"
    },
    "MCQ List": {
        "prefix": "mcqlist",
        "body": [
            "mcq = [",
            "    {",
            "        \"question\": r\"${1:Sample MCQ question?}\",",
            "        \"options\": [",
            "            r\"${2:Option A}\",",
            "            r\"${3:Option B}\",",
            "            r\"${4:Option C}\",",
            "            r\"${5:Option D}\"",
            "        ],",
            "        \"answer\": r\"${2:Option A}\",",
            "        \"marks\": ${6:1}",
            "    }",
            "]"
        ],
        "description": "Insert complete MCQ list structure"
    },
    "Subjective List": {
        "prefix": "subjectivelist",
        "body": [
            "subjective = [",
            "    {",
            "        \"question\": r\"${1:Sample subjective question.}\",",
            "        \"answer\": r\"${2:Sample answer or solution.}\",",
            "        \"marks\": ${3:5}",
            "    }",
            "]"
        ],
        "description": "Insert complete subjective list structure"
    }
}